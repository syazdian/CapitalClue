{
  "DetailApiBaseAddress": "$DETAIL_APIBASE_ADDRESS",
  "ConnectionStrings": {
    "SqlServer": "$CONNECTIONSTRINGS_SQLSERVER"
  },
  "AzureAd": {
    "Instance": "$AZUREAD_INSTANCE",
    "Domain": "$AZUREAD_DOMAIN",
    "TenantId": "$AZUREAD_TENANTID",
    "ClientId": "$AZUREAD_CLIENTID",
    "CallbackPath": "$AZUREAD_CALLBACKPATH",
    "Scopes": "$AZUREAD_SCOPES"
  },
  "AllowedHosts": "*",

  //TOKEN
  "ApiToken": {
    "token_url": "$APITOKEN_TOKENURL",
    "client_id": "$APITOKEN_CLIENTID",
    "client_secret": "$APITOKEN_CLIENTSECRET",
    "grant_type": "$APITOKEN_GRANTTYPE",
    "scope": "$APITOKEN_SCOPE"
  }
}